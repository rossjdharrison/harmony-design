{
  "id": "domain:ui",
  "type": "domain",
  "name": "UI Rendering Domain",
  "namespace": "ui",
  "description": "Handles UI component rendering, DOM manipulation, and user interaction. Implements Web Components with shadow DOM and manages component lifecycle.",
  "boundedContext": null,
  "responsibilities": [
    "Web Component registration and lifecycle",
    "Shadow DOM management",
    "Event delegation and handling",
    "Component state synchronization",
    "Accessibility and ARIA attributes"
  ],
  "interfaces": {
    "commands": [
      "RegisterComponent",
      "RenderComponent",
      "UpdateComponentState",
      "DestroyComponent"
    ],
    "events": [
      "ComponentRegistered",
      "ComponentRendered",
      "ComponentStateChanged",
      "ComponentDestroyed",
      "UserInteraction"
    ],
    "queries": [
      "getComponentState",
      "getRegisteredComponents",
      "findComponentById"
    ]
  },
  "dependencies": [],
  "implementation": {
    "language": "javascript",
    "target": "web",
    "path": "harmony-ui"
  },
  "performance": {
    "budget": {
      "memory": "10MB",
      "latency": "16ms"
    },
    "critical": true
  },
