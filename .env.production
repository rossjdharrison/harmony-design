# Production Environment Configuration
NODE_ENV=production

# Production API (should be overridden by deployment)
API_URL=https://api.harmony-design.com
VITE_API_URL=https://api.harmony-design.com
WS_URL=wss://api.harmony-design.com
VITE_WS_URL=wss://api.harmony-design.com

# Production logging
DEBUG=false
VITE_DEBUG=false
LOG_LEVEL=warn
VITE_LOG_LEVEL=warn

# Enable analytics in production
ENABLE_ANALYTICS=true
VITE_ENABLE_ANALYTICS=true

# Disable experiments in production
ENABLE_EXPERIMENTS=false
VITE_ENABLE_EXPERIMENTS=false

# Optimized audio settings
AUDIO_BUFFER_SIZE=256
VITE_AUDIO_BUFFER_SIZE=256